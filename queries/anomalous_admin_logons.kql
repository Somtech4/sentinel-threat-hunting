// T1078 - Valid Accounts
// Privileged account logons outside business hours
SecurityEvent
| where EventID == 4624
| where Account has "admin" or Account == "administrator"
| extend Hour = hour_of_day(TimeGenerated)
| where Hour between (0 .. 7) or Hour between (20 .. 23)
| project TimeGenerated, Account, Computer, IpAddress, Hour
